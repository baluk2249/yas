pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                sh 'mvn clean package -pl cart -am'
                sh 'ls -l cart/target'  
            }
        }
        stage('Scan'){
            withSonarQubeEnv(installationName: 'sq') {
                sh 'mvn org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -pl cart -am'
            }
        }
    }
}